<resources>
    <style name="Theme.Hologreen" parent="@android:style/Theme.Holo">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_hologreen</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Hologreen</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Hologreen</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Hologreen</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Hologreen</item>
        <item name="android:actionBarStyle">@style/ActionBar.Transparent.Hologreen</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_hologreen</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_hologreen</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Hologreen</item>

        <item name="android:editTextBackground">@drawable/hologreen_edit_text_holo_dark</item>
        <item name="android:textColorHighlight">#9999cc00</item>
        <item name="android:textSelectHandleLeft">@drawable/hologreen_text_select_handle_left</item>
        <item name="android:textSelectHandleRight">@drawable/hologreen_text_select_handle_right</item>
        <item name="android:textSelectHandle">@drawable/hologreen_text_select_handle_middle</item>
        <item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewHologreen</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/hologreen_btn_check_holo_dark</item>
        <item name="android:listChoiceIndicatorSingle">@drawable/hologreen_btn_radio_holo_dark</item>
        <item name="android:buttonStyle">@style/ButtonHologreen</item>
        <item name="android:imageButtonStyle">@style/ImageButtonHologreen</item>
        <item name="android:dropDownSpinnerStyle">@style/SpinnerHologreen</item>
        <item name="android:progressBarStyleHorizontal">@style/ProgressBarHologreen</item>
        <item name="android:seekBarStyle">@style/SeekBarHologreen</item>
        <item name="android:ratingBarStyle">@style/RatingBarHologreen</item>
        <item name="android:ratingBarStyleIndicator">@style/RatingBarBigHologreen</item>
        <item name="android:ratingBarStyleSmall">@style/RatingBarSmallHologreen</item>
        <item name="android:buttonStyleToggle">@style/ToggleHologreen</item>
        <item name="android:listChoiceBackgroundIndicator">@drawable/hologreen_list_selector_holo_dark</item>
        <item name="android:activatedBackgroundIndicator">@drawable/hologreen_activated_background_holo_dark</item>
        <item name="android:fastScrollThumbDrawable">@drawable/hologreen_fastscroll_thumb_holo</item>
    </style>

    <style name="ActionBar.Solid.Hologreen" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_hologreen</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_hologreen</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_hologreen</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Hologreen</item>
    </style>

    <style name="ActionBar.Transparent.Hologreen" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_hologreen</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Hologreen</item>
    </style>

    <style name="PopupMenu.Hologreen" parent="@android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_hologreen</item>	
    </style>

    <style name="DropDownListView.Hologreen" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_hologreen</item>
    </style>

    <style name="ActionBarTabStyle.Hologreen" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_hologreen</item>
    </style>

    <style name="DropDownNav.Hologreen" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_hologreen</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_hologreen</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_hologreen</item>
    </style>
    
    <style name="ProgressBar.Hologreen" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_hologreen</item>
    </style>
    
    <style name="ActionButton.CloseMode.Hologreen" parent="@android:style/Widget.Holo.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_hologreen</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Hologreen.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Hologreen</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Hologreen</item>
    </style>

  <style name="AutoCompleteTextViewHologreen" parent="android:Widget.Holo.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/hologreen_list_selector_holo_dark</item>
	    <item name="android:background">@drawable/hologreen_edit_text_holo_dark</item>
  </style>

  <style name="ButtonHologreen" parent="android:Widget.Holo.Button">
	  <item name="android:background">@drawable/hologreen_btn_default_holo_dark</item>
  </style>

  <style name="ImageButtonHologreen" parent="android:Widget.Holo.ImageButton">
	  <item name="android:background">@drawable/hologreen_btn_default_holo_dark</item>
  </style>

  <style name="SpinnerHologreen" parent="android:Widget.Holo.Spinner">
      <item name="android:background">@drawable/hologreen_spinner_background_holo_dark</item>
      <item name="android:dropDownSelector">@drawable/hologreen_list_selector_holo_dark</item>
  </style>

  <style name="TabHologreen">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/hologreen_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextHologreen">
	  <item name="android:textColor">#ffffff</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarHologreen" parent="android:Widget.Holo.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/hologreen_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/hologreen_progress_indeterminate_horizontal_holo_dark</item>
  </style>

  <style name="SeekBarHologreen" parent="android:Widget.Holo.SeekBar">
      <item name="android:progressDrawable">@drawable/hologreen_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/hologreen_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:thumb">@drawable/hologreen_scrubber_control_selector_holo_dark</item>
  </style>

  <style name="RatingBarHologreen" parent="android:Widget.Holo.RatingBar">
      <item name="android:progressDrawable">@drawable/hologreen_ratingbar_full_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/hologreen_ratingbar_full_holo_dark</item>
  </style>

  <style name="RatingBarBigHologreen" parent="android:Widget.Holo.RatingBar.Indicator">
      <item name="android:progressDrawable">@drawable/hologreen_ratingbar_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/hologreen_ratingbar_holo_dark</item>
  </style>

  <style name="RatingBarSmallHologreen" parent="android:Widget.Holo.RatingBar.Small">
      <item name="android:progressDrawable">@drawable/hologreen_ratingbar_small_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/hologreen_ratingbar_small_holo_dark</item>
  </style>

  <style name="ToggleHologreen" parent="android:Widget.Holo.Button.Toggle">
      <item name="android:background">@drawable/hologreen_btn_toggle_holo_dark</item>
  </style>
</resources>
